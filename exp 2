a=int(input("Enter number 1:"))                    
b=int(input("Enter number 2:"))                   
c=4
rand=a&b
ror=a/b
rnand=~rand
print("The cycle value is ",c)
ins=int(input("Enter number of instructions:"))
print("The performance measure:",ins/c)
print("result and=",rand)
print("result or=",ror)
print("result nand=",rnand)
